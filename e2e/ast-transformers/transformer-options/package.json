{
  "dependencies": {
    "@formatjs/ts-transformer": "^3.10.1",
    "react": "^18.2.0",
    "react-intl": "^6.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[tj]sx?$": ["<rootDir>/../../../legacy.js", {
        "astTransformers": {
          "before": [
            {
              "path": "<rootDir>/node_modules/@formatjs/ts-transformer/ts-jest-integration",
              "options": {
                "removeDefaultMessage": true
              }
            }
          ]
        }
      }]
    }
  }
}
