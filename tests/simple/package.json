{
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "../../preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "Hello.ts",
      "NullCoverage.js"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
