{
  "scripts": {
    "test": "shx rm -rf coverage && jest --coverage --no-cache",
    "test:series": "shx rm -rf coverage && jest -i --coverage --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "\\.test\\.ts$",
    "testResultsProcessor": "../../coverageprocessor.js",
    "transform": {
      ".(ts|tsx)": "../../preprocessor.js"
    }
  },
  "dependencies": {
    "@types/jest": "19.2.0",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "shx": "0.2.2",
    "ts-jest": "19.0.0",
    "typescript": "2.2.1"
  },
  "engines": {
    "node": "7.7.2",
    "npm": "4.3.0"
  },
  "private": true
}
